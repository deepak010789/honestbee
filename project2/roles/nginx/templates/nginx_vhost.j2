server {
    listen 80;
    server_name _;
    access_log /var/log/nginx/access.log;
    error_log /var/log/nginx/error.log info;


    location  ~ \.(aspx|php|jsp|cgi)$ {
      return 410;
    }

    location / {
      alias /var/www/honestbee/current/static/;
      gzip_static on;
      expires max;
      add_header Cache-Control public;
    }
}
